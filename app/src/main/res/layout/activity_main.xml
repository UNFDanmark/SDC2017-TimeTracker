<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="software.unf.dk.timetracker.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView5"
            android:layout_column="0"
            android:layout_row="2"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="130dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            app:srcCompat="@android:color/background_dark"
            tools:layout_editor_absoluteX="42dp"
            tools:layout_editor_absoluteY="432dp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="createClassification"
            android:text="Add"
            tools:layout_editor_absoluteX="254dp"
            tools:layout_editor_absoluteY="274dp"
            android:layout_marginBottom="104dp"
            android:layout_above="@+id/button"
            android:layout_toRightOf="@+id/classificationText"
            android:layout_toEndOf="@+id/classificationText"
            android:layout_marginLeft="11dp"
            android:layout_marginStart="11dp" />

        <Button
            android:id="@+id/buttonSetter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="3"
            android:onClick="enter"
            android:text="Enter"
            tools:layout_editor_absoluteX="277dp"
            tools:layout_editor_absoluteY="50dp"
            android:layout_alignBaseline="@+id/svar"
            android:layout_alignBottom="@+id/svar"
            android:layout_alignLeft="@+id/button2"
            android:layout_alignStart="@+id/button2" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="3"
            android:text="What are you doing ?"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="22dp"
            android:layout_below="@+id/imageView2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="21dp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="historya"
            android:text="History"
            tools:layout_editor_absoluteX="264dp"
            tools:layout_editor_absoluteY="447dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="settingpath"
            android:text="Setting"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="447dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="piepath"
            android:text="Statistics"
            tools:layout_editor_absoluteX="136dp"
            tools:layout_editor_absoluteY="447dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_column="0"
            android:layout_row="3"
            android:text=" Name of catagory"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="247dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Create new catagory"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="225dp"
            android:layout_above="@+id/textView6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:id="@+id/classificationText"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView6"
            android:layout_column="0"
            android:layout_row="3"
            android:layout_toEndOf="@+id/editText"
            android:layout_toRightOf="@+id/editText"
            android:ems="10"
            android:inputType="textPersonName"
            android:singleLine="false"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="269dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/svar"
            android:layout_column="0"
            android:layout_row="15"
            android:text=" Choose catagory"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="114dp" />

        <EditText
            android:id="@+id/svar"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView7"
            android:ems="10"
            android:inputType="textPersonName" />
    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
